.tab-type02 {
    background: #f1f2f8;
    border-radius: 16px;
    padding: 1.3rem;
    padding-bottom: 2rem;
}

.tab-type02 .tab {
    margin-bottom: 26px;
    text-align: center;
}

.tab-type02 .tab:after {
    display: block;
    content: "";
    clear: both;
}

.tab-type02 .tab>div {
    display: inline-block;
}

.tab-type02 .tab>div>a {
    display: block;
    padding: 10px 20px;
    font-size: 16px;
    color: #9a9dac;
    font-weight: 600;
}

.tab-type02 .tab>div>a.active {
    color: #364085;
    border-bottom: 3px solid #364085;
    position: relative;
}

.tab-type02 .tab>div>a.active:after {
    display: block;
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    bottom: -9px;
    left: 50%;
    margin-left: -6px;
    border-bottom: 0;
    border-top: 6px solid #364085;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
}

.tab-type02 .tab-container {
    border-radius: 16px;
    background: #fff;
    padding: 1.5rem;
    box-shadow: 0px 4px 10px rgba(0, 0, 0, .2);
    min-height: 250px;
    display:flex;
    align-items:center;
    justify-content:center;
    width:100%;
}

.tab-type02 .tab-container .inputtxt02 {
    font-size: 18px;
    width: 100%;
    height: 40px;
    border: 0;
    border: 1px solid #d7daf1;
    padding: 11px 10px;
    color: #666;
}



    
.login-type {position: absolute;right: 0;top: 14px;display: flex;}
.login-type a {color: #fff;font-size: 18px;font-weight: 300;margin-left: 24px; position: relative;}
.login-type a:after {content: ""; display: block; width: 1px; height: 10px; background: rgba(255, 255, 255, 0.42); position: absolute;left: -13px; top: 6px}
.login-type a:first-of-type:after {display: none; }
.login-type a.on:before { content: "";display: block;width: 6px;height: 6px;background:#FFA929;border-radius: 600px;position: absolute;right: -7px;top: -4px;}
.login-type a span {position: relative;}
.login-type a span:before {transition: all 0.3s ease;content: "";display: block; width: 0%; height: 2px; background:#FFA929;    left: 0; position: absolute; bottom: -4px;}
.login-type a:hover span:before, .login-type a.on span:before {width: 100%; transition: all 0.3s ease;}





@keyframes zoomin {
		0%{ transform:scale(1.2); -webkit-transform:scale(1.2); }
		100%{ transform:scale(1); -webkit-transform:scale(1); }
	}
	
@-webkit-keyframes zoomin {
		0%{ transform:scale(1.2); -webkit-transform:scale(1.2); }
		100%{ transform:scale(1); -webkit-transform:scale(1); }
	}

