/***spinner***/
.spinner {outline: none;-moz-appearance: textfield;}
.spinner::-webkit-inner-spin-button, 
.spinner::-webkit-outer-spin-button { -webkit-appearance: none;appearance: none;}
.spinner-inner .ui-spinner{ position: relative;  display: inline-block; }
.spinner-inner .ui-spinner a.ui-spinner-button { background: none; border-left: 1px solid #ddd;  border-top: 1px solid #ddd; width: 33px; height: 50%; position: absolute; right: 0;    cursor: pointer;transition: all 0.3s ease;}
.spinner-inner .ui-spinner a.ui-spinner-button:hover{background: rgba(234, 234, 234, 0.55);}
.spinner-inner .ui-spinner a.ui-spinner-button:first-of-type {top: 0;}
.spinner-inner .ui-spinner a.ui-spinner-button:nth-of-type(2) {bottom: 0;}
.spinner-inner .ui-button-text-only .ui-button-text {padding: 1px 0;position: absolute; left: 50%;margin-left: -9px;}
.spinner-inner .ui-spinner a.ui-spinner-button:first-of-type .ui-button-text {margin-top: -5px;}
.ui-spinner-button:first-of-type:after,
.spinner-inner .ui-spinner a.ui-spinner-button:nth-of-type(2):after { position: absolute;left: 50%;    transform: translate(-50%, -5px);font-family: 'Material Icons';font-size: 19px;height: 0;/* Support for all WebKit browsers. */-webkit-font-smoothing: antialiased; /* Support for Safari and Chrome. */ text-rendering: optimizeLegibility;/* Support for Firefox. */ -moz-osx-font-smoothing: grayscale;/* Support for IE. */font-feature-settings: 'liga';top: -1px;}
.ui-spinner-button:first-of-type:after {content: "arrow_drop_up";}
.spinner-inner .ui-spinner a.ui-spinner-button:nth-of-type(2):after {content: "arrow_drop_down";}




