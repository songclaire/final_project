/*** align ***/
.right{text-align:right!important;}
.left{text-align:left!important;}
.center{text-align:center!important;}


/*** text color ***/
.highlight {display: inline-block; vertical-align: middle;margin: 0;}
.blu-txt {color: #0080de;}
.org-txt {color: #fd5621; }
.grn-txt {color: #4eaf4e;}
.red-txt {color: #f04054;}
.pur-txt {color: #5122ac;}
.asterisk  { font-size: 18px; vertical-align: middle; position: relative; top: 3px;}


/*** pagination   ***/
.pagination {justify-content: center;}
.active>.page-link, .page-link.active { background: #5D5AEF; border: 1px solid #5D5AEF;}
.page-link { margin: 5px;color: #000;}


/*** tob-box  ***/
.tob-box {background: rgba(255, 245, 234, 1);border: 1px solid rgba(236, 236, 236, 1); padding: 25px 35px; margin: 15px 0;}
.tob-box p {margin-bottom: 5px;}
.tob-box .tit { display: block; font-size: 16px;font-weight: 600;margin-bottom: 11px;}
    



/*** table ***/
table {border-top: 1px solid #b2b2b2;font-size: 13px; width: 100%;    table-layout: fixed;}
caption {font-size: 0;}
table i { font-size: 12px;}
.center td {text-align: center;}
/*tbl*/
.tbl tr {border-bottom: 1px solid #dde0e8;}
.tbl th{padding:9px 0;}
.tbl thead th,.tbl tbody th  {background-color: #ecf0f9;}
.tbl td{padding: 6px;; background-color:#fff; vertical-align:middle; white-space: nowrap; text-overflow: ellipsis;overflow: hidden;border: 1px solid #ddd;height: 29px;}
.tbl tr:first-of-type  td {border-top: 0;}
.tbl thead th,.tbl tbody th{min-width: 50px; vertical-align: middle;color: #2f3032;text-align: center; font-weight: 600; border: 1px solid #ddd;border-top: 0;}
.tbl tfoot td{border-bottom:none; background:#ffeded;}
/* scroll tbl */
.table-scrollable {
  --max-height: 90vh;
  margin-bottom: 1rem;
  overflow: hidden;
  border: 1px solid #dee2e6;
}
.table-scrollable > .table-responsive {
  width: auto;
  max-height: 50vh;
  margin: -1px;
  overflow: auto;
}
.table-scrollable > .table-responsive > .table {
  margin-bottom: 0;
}
.table-scrollable__fixed-y th {
  position: sticky;
  top: 0;
  z-index: 10;
  background: #ddd;
}
.table-scrollable__fixed-y th::after {
  position: absolute;
  top: 100%;
  right: -0.5px;
  left: -0.5px;
  height: 8px;
  content: "";
  opacity: 0;
  pointer-events: none;
  transition-duration: 250ms;
  transition-property: opacity;
}
.table-scrollable--scrolled-y .table-scrollable__fixed-y {
  background-color: inherit;
}
.table-scrollable--scrolled-y .table-scrollable__fixed-y th {
  background-color: inherit;
}
.table-scrollable--scrolled-y .table-scrollable__fixed-y th::after {
  opacity: 1;
  background: linear-gradient(rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.06) 50%, transparent 80%);
}
.table-scrollable__fixed-x {
  position: sticky;
  left: 0;
  z-index: 5;
}
.table-scrollable__fixed-x::after {
  position: absolute;
  top: -0.5px;
  bottom: -0.5px;
  left: 100%;
  width: 8px;
  content: "";
  opacity: 0;
  pointer-events: none;
  transition-duration: 250ms;
  transition-property: opacity;
}
.table-scrollable--scrolled-x .table-scrollable__fixed-x {
  box-shadow: 0 0 1px #dee2e6;
}
.table-scrollable--scrolled-x .table-scrollable__fixed-x::after {
  opacity: 1;
  background: linear-gradient(to right, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.06) 50%, transparent 80%);
}



















/***photo-wrap  ***/
.photo-wrap  {display: flex;}
.photo-wrap .photo {width: 100px;height: 120px;background: #ddd;margin-right: 10px; }
.photo-wrap .tbl-wrap {width: calc(100% - 110px);}
