/*util*/
.util { display: flex; position: absolute; right: 25px; top: 50%; margin-top: -20px;align-items: center;}

.util > * {margin-left: 15px;}

.util .login-session a{margin-left: 5px;color: #222;}

.util .dropdown .profile {display: inline-block;width: 40px;  height: 40px;  background: #ddd; border-radius: 600px; position: absolute; left: 0;}
.util .dropdown-toggle {color: #222; border: 0; text-decoration: none;    padding-left: 47px;    display: inline-block;}
.util .dropdown-toggle::after {display: none;}
.util .dropdown .person-name {display: block; font-size: 15px;}
.util .dropdown .person-info {font-size: 13px; color: #919191;}
    
.util .icon {display: inline-block; min-width: 20px; height: 20px; background: url(../images/util-icon.png) no-repeat;overflow: visible;}
.util .icon.bell {background-position: 0 0;}
.util .icon.icon02 {background-position: 0 -31px;}
.util .bell .num { display: inline-block; margin-top: -23px; margin-left: 8px;min-width: 5px; height: 5px; background: #ff0000; border-radius: 600px; padding: 1px 3px; font-size: 11px; color: #fff;vertical-align: middle; text-align: center;white-space: nowrap; }
.util .bell.no_anime {transform-origin: top;animation: bell 2s infinite linear;}
@keyframes bell{
  0%, 50%{
    transform: rotate(0deg);
	}
  5%, 15%, 25%, 35%, 45% {
    transform: rotate(3deg);
  }
  10%, 20%, 30%, 40% {
    transform: rotate(-3deg);
  }
}
.bell.no_anime .num {
  box-shadow: 0 0 0 rgba(204,169,44, 0.4);
  animation: pulse 2s infinite;
}

@-webkit-keyframes pulse {
  0% {
    -webkit-box-shadow: 0 0 0 0 rgba(255, 0, 0, 0.46);
  }
  70% {
      -webkit-box-shadow: 0 0 0 10px rgba(255, 0, 0, 0);
  }
  100% {
      -webkit-box-shadow: 0 0 0 0 rgba(255, 0, 0, 0);
  }
}
@keyframes pulse {
  0% {
    -moz-box-shadow: 0 0 0 0 rgba(255, 0, 0, 0.46);
    box-shadow: 0 0 0 0 rgba(255, 0, 0, 0.4);
  }
  70% {
      -moz-box-shadow: 0 0 0 10px rgba(255, 0, 0, 0);
      box-shadow: 0 0 0 10px rgba(255, 0, 0, 0);
  }
  100% {
      -moz-box-shadow: 0 0 0 0 rgba(255, 0, 0, 0);
      box-shadow: 0 0 0 0 rgba(255, 0, 0, 0);
  }
}

