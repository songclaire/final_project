/* scroll tbl */
.table-scrollable {
  --max-height: 90vh;
  margin-bottom: 1rem;
  overflow: hidden;
  border: 1px solid #dee2e6;
}
.table-scrollable > .table-responsive {
  width: auto;
  max-height: 50vh;
  margin: -1px;
  overflow: auto;
}
.table-scrollable > .table-responsive > .table {
  margin-bottom: 0;
}
.table-scrollable__fixed-y th {
  position: sticky;
  top: 0;
  z-index: 10;
  background: #ddd;
}
.table-scrollable__fixed-y th::after {
  position: absolute;
  top: 100%;
  right: -0.5px;
  left: -0.5px;
  height: 8px;
  content: "";
  opacity: 0;
  pointer-events: none;
  transition-duration: 250ms;
  transition-property: opacity;
}

.table-scrollable--scrolled-y .table-scrollable__fixed-y th::after {
  opacity: 1;
  background: linear-gradient(rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.06) 50%, transparent 80%);
}
.table-scrollable__fixed-x {
  position: sticky;
  left: 0;
  z-index: 5;
}
.table-scrollable__fixed-x::after {
  position: absolute;
  top: -0.5px;
  bottom: -0.5px;
  left: 100%;
  width: 8px;
  content: "";
  opacity: 0;
  pointer-events: none;
  transition-duration: 250ms;
  transition-property: opacity;
}
.table-scrollable--scrolled-x .table-scrollable__fixed-x {
  box-shadow: 0 0 1px #dee2e6;
}
.table-scrollable--scrolled-x .table-scrollable__fixed-x::after {
  opacity: 1;
  background: linear-gradient(to right, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.06) 50%, transparent 80%);
}






