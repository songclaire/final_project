/*** breadcrumb  ***/
.cont-navi{position:absolute; top:0px; right:0;}
.cont-navi span{display:inline-block; vertical-align:middle; color:#b8b8b8; font-size:11px;}
.cont-navi strong{display:inline-block; vertical-align:middle; color:#888; font-weight:normal;}
.cont-navi span:before, .cont-navi strong:before{display:inline-block;font-family:'bootstrap-icons' ;  ; content:"\f285"; margin-right:3px;}
.cont-navi span:first-child{width:13px; height:13px; overflow:hidden; line-height:99; }
.cont-navi span:first-child:before{display:none;}

/*** cont-box ***/
.cont-title {position: relative;}
.cont-title h2, .title h2{ display: inline-block;font-size: 27px; color: #202020; font-weight: 500; line-height: 1; margin-bottom: 22px;}
.cont-title .star {margin-left: 8px; vertical-align: bottom;display: inline-block; width: 20px; height: 22px; background: url(../images/star.png) no-repeat; }
.cont-title .star.on {background: url(../images/star-ov.png) no-repeat; transition: all 0.3s ease;}
.white-box { background: #fff; padding: 25px 32px; border-radius: 10px;}
.cont-box-inner:not(:first-of-type) {padding-top: 35px;}
.cont-box-inner:first-of-type { padding-top: 0;}



/***  title  ***/
.title {position:relative;min-height: 37px;}
.title h3, .title h4 {display: inline-block;font-weight: 600;}
.title h3, .title h4 {font-size:20px;}
.title  > .box-btn { margin-top: 0;position: absolute;right: 0; top: -3px;}
.box-btn > * { float: left; margin: 0 2px; vertical-align: middle;}
.box-btn > *, .box-btn > * > .btn {min-height: 35px;}
.box-btn:after{content: ""; display: block; clear: both;}
.right-part .box-btn > * {float: none;}
.right-part .box-btn > * {margin: 0;}
.right-part .box-btn > *, .right-part .box-btn > * > .btn {min-height: inherit;}

.title .total{ margin-left: 10px;min-width: 39px;    height: 22px; line-height: 22px;padding: 0 5px;border-radius: 600px;background: #f14256; display: inline-block;text-align: center;padding: 1px 9px;color: #fff;font-size: 12px;font-weight: 600;}
.title .total em{font-size: 12px;}

.title:after {content: ""; display: block; clear: both;}
.title .highlight-txt {float: left; padding-top: 5px;margin-bottom: 0;}
.title .right-part { text-align: right;position: relative; }
.title .right-part .search-form,
.title .right-part .box-btn  {display: inline-block;}
.title .right-part .box-btn {position: static;}



/***tab***/
/*tab-1dep*/
.tab-wrap .tab-1dep {padding-bottom: 5px;border-bottom: 1px solid #ddd; padding-left: 0;}
.tab-wrap .tab-1dep li{float:left; margin:0 2px;}
.tab-wrap .tab-1dep:after{content:''; display:block; clear:both;}
.tab-wrap .tab-1dep li a{display: block; padding: 6px 13px; background: #eaeaea; color: rgba(0, 0, 0, 0.5);font-weight: 600;height: 30px;border-radius: 3px;margin-left: -1px;}
.tab-wrap .tab-1dep li a:hover{color:#666;}
.tab-wrap .tab-1dep li.on a{background:#5d5aef; cursor:default; color:#fff; font-weight:bold;}
.tab-wrap .tab-1dep-in > li{display:none;}
.tab-wrap .tab-1dep-in > li:first-child{display:block;}
.tab-1dep-button {position: relative;}
.tab-1dep-button  .box-btn {position: absolute; top: 0; right: 0; margin-top: 0;}
