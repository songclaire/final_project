.navbar-brand span {display: inline-block;width: 165px;}
/* sidemenu  tab */
.sb-sidenav .tab-wrap {display: flex; margin: 10px 11px 0 11px; justify-content: center;  align-items: center; text-align: center;}
.sb-sidenav .tab-wrap > a{ background: #fff; display: inline-block; width: 48%; padding: 8px; color: #5d5aef; text-decoration: none;  font-weight: 600; font-size: 13px; transition: all 0.3s ease; }
.sb-sidenav .tab-wrap > a:hover {color: #5d5aef; }
.sb-sidenav .tab-wrap > a.active { background: #131C38; color: #fff;  }
/* sb-sidenav */  
.sb-nav-link-icon {padding-right: 8px;padding-top: 3px;font-size: 12px;}
.sb-sidenav-common  {padding: 15px; padding-bottom: 13px;padding-left: 0;padding-right: 0;min-height: 200px;}
.sb-sidenav-common .nav-link {color: #fff; display: flex;}
.sb-sidenav-common .nav-link:hover {background: #4c49ff;}
/* sb-nav- */
.sb-nav-fixed #layoutSidenav #layoutSidenav_nav {width: 225px; height: 100vh; z-index: 1038;}
